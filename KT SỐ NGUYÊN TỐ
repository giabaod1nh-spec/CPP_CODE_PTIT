//Lap voi buoc nhay 1 
#include<isotream>
using namespace std ; 
bool ktsonguyento(int n ) {
if(n<2){
return 0 ; 
}
for(int i = 2 ; i <= n/2 ; i ++){
 if(n % i  == 0 ){
return 0 ; 
}
 }
return 1 ; 
}
int main(){
int n ; 
cin >> n ; 
cout<<ktsonguyento(n) ; 
}
//Lap voi buoc nhay 2 
#include<iostream>
uisng namespace std ; 
bool ktsonguyento(int n ){
if(n < 2){
return 0 ; 
}
if(n==2){
return 1 ; 
}
for(int i = 3 ; i <= n/2 ; i = i + 2){
if(n % i == 0 ){
return  0 ; 
}
 }
return 1 
}
int main(){
int n ; 
cin >> n ; 
cout<<ktsonguyento(n) ; 
}

